#include<stdio.h>
int main()
{
	int num1,num2,r,i;
	printf("Enter the min range");
	scanf("%d",&num1);
	printf("Enter the max range");
	scanf("%d",&num2);
	printf("\n display even numbers");
	for(i=num1;i<= num2;i++)
	{
		r=i%2;
		if(r==0)
		{
		printf("\n %d",i);
	}
	}
	printf("\n Display odd numbers");
	for(i=num1;i<=num2;i++)
	{
		r=i%2;
		if(r==1)
		printf("\n %d",i);
	}
	return 0;
}
